<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WpfRegister">

    <Color x:Key="BackgroundGrey">#efefef</Color>
    <SolidColorBrush x:Key="BackgroundGreyBrush" Color="{StaticResource BackgroundGrey}"/>

    <Color x:Key="BackgroundLightGrey">#dfdfdf</Color>
    <SolidColorBrush x:Key="BackgroundLightGreyBrush" Color="{StaticResource BackgroundLightGrey}"/>

    <Color x:Key="TextColor">#ffffff</Color>
    <SolidColorBrush x:Key="TextColorBrush" Color="{StaticResource TextColor}"/>
</ResourceDictionary>